{
  "name": "solidity-nft",
  "version": "1.0.0",
  "description": "Generate nft from Solidity",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prettier:contracts": "npx prettier --write 'contracts/**/*.sol'",
    "lint:contracts": "npx solhint 'contracts/**/*.sol'",
    "truffle:compile": "npx truffle compile",
    "truffle:migrate": "npx truffle migrate",
    "build": "npm run prettier:contracts && npm run lint:contracts && npm run truffle:compile",
    "test": "npx truffle test"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.3.0",
    "@truffle/hdwallet-provider": "^1.4.0",
    "chai": "^4.3.4",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "ethereumjs-util": "^7.0.10",
    "line-reader": "^0.4.0",
    "openzeppelin-test-helpers": "^0.5.1",
    "run": "^1.4.0",
    "truffle": "^5.3.6",
    "verify": "^0.1.2",
    "web3": "^1.3.5"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.7",
    "truffle-plugin-verify": "^0.5.8",
    "solhint": "^3.3.2",
    "solhint-plugin-prettier": "0.0.5"
  },
  "author": "",
  "license": "ISC"
}
